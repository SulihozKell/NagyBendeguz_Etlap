<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
      fx:controller="hu.petrik.etlap.controllers.KategoriaFelvetelController" prefHeight="100.0" prefWidth="300.0">
    <GridPane alignment="TOP_RIGHT">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="232.0" minWidth="5.0" prefWidth="150.0"/>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="495.0" minWidth="10.0" prefWidth="332.0"/>
            <ColumnConstraints/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints maxHeight="30.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints maxHeight="70.0" minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0"/>
        </padding>
        <Button alignment="CENTER" minHeight="19.0" minWidth="50.0" mnemonicParsing="false"
                onAction="#onKategoriaFelvetelButtonClick" prefHeight="26.0" prefWidth="400.0" text="Hozzáad"
                textAlignment="CENTER" GridPane.columnSpan="2" GridPane.rowIndex="1"/>
        <Label alignment="TOP_RIGHT" prefHeight="17.0" prefWidth="105.0" text="Név" textAlignment="RIGHT">
            <padding>
                <Insets right="5.0"/>
            </padding>
        </Label>
        <TextField fx:id="inputNev" maxWidth="250.0" prefWidth="250.0" promptText="Kategória neve"
                   GridPane.columnIndex="1"/>
    </GridPane>
</VBox>
